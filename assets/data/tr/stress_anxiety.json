    { "id": "sa_403", "text": "Her korku öğretti, artık bırakabilirim.", "gender": "any" },
    { "id": "sa_404", "text": "Duygularıma direnmiyorum, onlarla akıyorum.", "gender": "any" },
    { "id": "sa_405", "text": "Bugün zihinsel baskıdan kurtuluyorum.", "gender": "female" },
    { "id": "sa_406", "text": "Vücudumu saran her endişe dağılıyor.", "gender": "any" },
    { "id": "sa_407", "text": "Her solukta özgürleşiyor, hafifleşiyorum.", "gender": "any" },
    { "id": "sa_408", "text": "Bugün içimde büyüyen huzur beni kucaklıyor.", "gender": "any" },
    { "id": "sa_409", "text": "Zihinsel bulanıklık netleşiyor.", "gender": "any" },
    { "id": "sa_410", "text": "Kalbimde yaşayan dinginlik genişliyor.", "gender": "any" },
    { "id": "sa_411", "text": "Bugün endişeleri nazikçe bırakıyorum.", "gender": "any" },
    { "id": "sa_412", "text": "Her düşünce gelip geçer, ben burada kalırım.", "gender": "any" },
    { "id": "sa_413", "text": "Duygularımla savaşmak yerine barışıyorum.", "gender": "any" },
    { "id": "sa_414", "text": "Bugün zihinsel ağırlık yerine hafiflik seçiyorum.", "gender": "any" },

    { "id": "sa_415", "text": "Şu anki hislerim zorlayıcı olsa da geçici.", "gender": "any" },
    { "id": "sa_416", "text": "Bugün kendimle daha yumuşak bir ilişki kurabilirim.", "gender": "any" },
    { "id": "sa_417", "text": "Her şeyi çözmek zorunda değilim.", "gender": "any" },
    { "id": "sa_418", "text": "Zihnim biraz mola verebilir.", "gender": "any" },
    { "id": "sa_419", "text": "Bu anla baş etmek için yeterliyim.", "gender": "any" },

    { "id": "sa_420", "text": "Üzerimdeki baskının farkındayım ve bunu kabul ediyorum.", "gender": "any" },
    { "id": "sa_421", "text": "Yavaşlamak şu anda benim için daha iyi.", "gender": "any" },
    { "id": "sa_422", "text": "Düşüncelerim beni yönetmek zorunda değil.", "gender": "any" },
    { "id": "sa_423", "text": "Bugün kendime yüklenmemeyi seçiyorum.", "gender": "any" },
    { "id": "sa_424", "text": "İçimde toparlanmaya alan var.", "gender": "any" },

    {
      "id": "sa_425",
      "text": "Kaygı, henüz yaşanmamış şeylere verilen bir tepkidir. — Seneca",
      "gender": "any"
    },

    { "id": "sa_426", "text": "Şu an olanla kalabilirim.", "gender": "any" },
    { "id": "sa_427", "text": "Bedenimin verdiği tepkiler anlaşılabilir.", "gender": "any" },
    { "id": "sa_428", "text": "Bugün kendime biraz daha alan tanıyorum.", "gender": "any" },
    { "id": "sa_429", "text": "Zor bir günden geçiyorum ve bu insani.", "gender": "any" },
    { "id": "sa_430", "text": "Kontrol edemediklerimi bir süreliğine bırakabilirim.", "gender": "any" },

    {
      "id": "sa_431",
      "text": "Bazen durmak, ilerlemenin bir parçasıdır. — Thich Nhat Hanh",
      "gender": "any"
    },

    { "id": "sa_432", "text": "Kendimi toparlamak için zamana sahibim.", "gender": "any" },
    { "id": "sa_433", "text": "Şu an her şeyi net hissetmem gerekmiyor.", "gender": "any" },
    { "id": "sa_434", "text": "Gerginlik beni tanımlamaz.", "gender": "any" },
    { "id": "sa_435", "text": "Bugün daha sade bir tempoya geçebilirim.", "gender": "any" },
    { "id": "sa_436", "text": "Kendime karşı sert olmam gerekmiyor.", "gender": "any" },

    {
      "id": "sa_437",
      "text": "İnsan dayanabileceğinden fazlasını hissetmez. — Viktor Frankl",
      "gender": "any"
    },

    { "id": "sa_438", "text": "Bu hissin içinden geçiyorum, içinde kaybolmuyorum.", "gender": "any" },
    { "id": "sa_439", "text": "Bugün biraz daha yavaş olabilirim.", "gender": "any" },
    { "id": "sa_440", "text": "Zihnimdeki karmaşa azalabilir.", "gender": "any" },
    { "id": "sa_441", "text": "Kendime destek olmayı öğreniyorum.", "gender": "any" },
    { "id": "sa_442", "text": "Bu anın sonsuza kadar sürmesi gerekmiyor.", "gender": "any" },

    { "id": "sa_443", "text": "Şu anda olduğum halim yeterli.", "gender": "any" },
    { "id": "sa_444", "text": "Bugün beklentileri biraz düşürebilirim.", "gender": "any" },
    { "id": "sa_445", "text": "İçimde toparlanma kapasitesi var.", "gender": "any" },
    { "id": "sa_446", "text": "Bu süreci kendi hızımda yaşayabilirim.", "gender": "any" },
    { "id": "sa_447", "text": "Zor hisler gelip geçebilir.", "gender": "any" },
    { "id": "sa_448", "text": "Bugün kendimle kavga etmiyorum.", "gender": "any" },
    { "id": "sa_449", "text": "Bir adım geri çekilmek de bir seçim.", "gender": "any" },
    { "id": "sa_450", "text": "Şu an buradayım ve bu yeterli.", "gender": "any" },

  { "id": "sa_451", "text": "Vücudumdaki tüm baskı yumuşuyor.", "gender": "any" },
  { "id": "sa_452", "text": "Her nefeste daha özgür hissediyorum.", "gender": "any" },
  { "id": "sa_453", "text": "Bugün içimde filizlenen sakinlik büyüyor.", "gender": "any" },
  { "id": "sa_454", "text": "Zihinsel karmaşa sadeliğe dönüşüyor.", "gender": "any" },
  { "id": "sa_455", "text": "Kalbimde büyüyen rahatlık beni besliyor.", "gender": "any" },
  { "id": "sa_456", "text": "Bugün korkularımı onurlandırıp bırakıyorum.", "gender": "female" },
  { "id": "sa_457", "text": "Her endişe sisin içinde kaybolup gidiyor.", "gender": "any" },
  { "id": "sa_458", "text": "Duygularım dalgalansa da ben duruyorum.", "gender": "male" },
  { "id": "sa_459", "text": "Bugün zihinsel yükü omuzlarımdan atıyorum.", "gender": "any" },
  { "id": "sa_460", "text": "Vücudumdaki gerginlik erir, gider.", "gender": "any" },
  { "id": "sa_461", "text": "Her düşünce tüy kadar hafif.", "gender": "any" },
  { "id": "sa_462", "text": "Bugün içimde açılan pencere temiz hava getiriyor.", "gender": "any" },
  { "id": "sa_463", "text": "Zihinsel gürültü azaldıkça ben güçleniyorum.", "gender": "any" },
  { "id": "sa_464", "text": "Kalbim eskiden daha sakin çarpıyor.", "gender": "any" },
  { "id": "sa_465", "text": "Bugün kaygı yerini umuda bırakıyor.", "gender": "any" },
  { "id": "sa_466", "text": "Her korku beni yıkmaz, olgunlaştırır.", "gender": "any" },
  { "id": "sa_467", "text": "Duygularıma alan tanıyor, onları serbest bırakıyorum.", "gender": "any" },
  { "id": "sa_468", "text": "Bugün zihinsel baskı bana dokunmuyor.", "gender": "any" },
  { "id": "sa_469", "text": "Vücudumu saran endişe dağılıyor.", "gender": "any" },
  { "id": "sa_470", "text": "Her solukta özgürlüğe yaklaşıyorum.", "gender": "any" },
  { "id": "sa_471", "text": "Bugün içimde büyüyen dinginlik beni sarıyor.", "gender": "any" },
  { "id": "sa_472", "text": "Zihinsel bulanıklık berraklığa evrilıyor.", "gender": "any" },
  { "id": "sa_473", "text": "Korkuya rağmen nefes almak cesaret gerektirir. — Brené Brown", "gender": "any" },
  { "id": "sa_474", "text": "Bugün endişeleri yavaşça salıyorum.", "gender": "female" },
  { "id": "sa_475", "text": "Her düşünce akar gider, ben kalırım.", "gender": "any" },
  { "id": "sa_476", "text": "Duygularımla mücadele yerine uyum içindeyim.", "gender": "any" },
  { "id": "sa_477", "text": "Bugün zihinsel ağırlıktan arınıyorum.", "gender": "any" },
  { "id": "sa_478", "text": "Vücudumdaki tüm gerginlik yumuşuyor.", "gender": "any" },
  { "id": "sa_479", "text": "Sakinlik her fırtınadan sonra gelir ve her zaman gelir. — Maya Angelou", "gender": "any" },
  { "id": "sa_480", "text": "Bugün içimde filizlenen rahatlık beni kucaklıyor.", "gender": "any" },
  { "id": "sa_481", "text": "Zihinsel karmaşa sadeleşiyor.", "gender": "any" },
  { "id": "sa_482", "text": "Kalbimde büyüyen sükunet beni taşıyor.", "gender": "any" },
  { "id": "sa_483", "text": "Bugün korkularımı şefkatle bırakıyorum.", "gender": "any" },
  { "id": "sa_484", "text": "Endişe de bir duygu, ona izin verip salıyorum. — Jon Kabat-Zinn", "gender": "any" },
  { "id": "sa_485", "text": "Kalbimde yaşayan huzur genişliyor.", "gender": "any" },
  { "id": "sa_486", "text": "Her nefeste daha hafif, daha özgür.", "gender": "any" }

  ]
}